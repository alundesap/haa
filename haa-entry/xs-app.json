{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/sap/bc/ina",
		"destination": "HAA_BACKEND",
		"httpMethods": ["POST", "GET"],
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.Display"
	}, {
		"source": "/sap/bc/ina",
		"destination": "HAA_BACKEND",
		"httpMethods": ["OPTIONS"],
		"authenticationType": "none"
	}, {
		"source": "^/(.*)",
		"localDir": "resources",
		"authenticationType": "none"
	}]
}
